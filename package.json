{
	"name": "@makerspacedarmstadt/n8n-nodes-paperless",
	"version": "0.2.0",
	"description": "A N8N community node package for PaperlessNGX",
	"keywords": [
		"n8n-community-node-package",
		"paperless"
	],
	"license": "MIT",
	"homepage": "https://github.com/waza-ari/n8n-nodes-paperless",
	"author": {
		"name": "Daniel Herrmann",
		"email": "it@makerspace-darmstadt.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/waza-ari/n8n-nodes-paperless.git"
	},
	"engines": {
		"pnpm": ">=9.1",
		"node": ">=20.15"
	},
	"main": "index.js",
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/PaperlessNgxApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/PaperlessNgx/PaperlessNgx.node.js",
			"dist/nodes/PaperlessNgxUpload/PaperlessNgxUpload.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@parcel/watcher": "^2.5.1",
		"@types/node": "^24.3.1",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
